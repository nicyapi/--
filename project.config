[access]
	inheritFrom = chromiumos
[project]
	description = Parent for chromiumos/overlays/* projects, defines common ACLs.
[access "refs/for/refs/*"]
	exclusiveGroupPermissions = pushMerge
[submit]
	action = rebase if necessary
[access "refs/heads/staging"]
	owner = group mdb/chromeos-ci-oncalls
	push = group chromiumos-staging-bot
	submit = group chromiumos-staging-bot
	submitAs = group chromiumos-staging-bot
